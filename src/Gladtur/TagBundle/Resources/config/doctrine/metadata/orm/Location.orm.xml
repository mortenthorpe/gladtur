<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Location" table="location">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="readableName" type="string" column="readable_name" length="255"/>
    <field name="published" type="boolean" column="published"/>
    <field name="addressZip" type="string" column="address_zip" length="20"/>
    <field name="addressCountry" type="string" column="address_country" length="255"/>
    <field name="addressCity" type="string" column="address_city" length="255"/>
    <field name="addressStreet" type="string" column="address_street" length="255"/>
    <field name="addressExtd" type="string" column="address_extd" length="255"/>
    <field name="phone" type="string" column="phone" length="20"/>
    <field name="mail" type="string" column="mail" length="255"/>
    <field name="homepage" type="string" column="homepage" length="255"/>
    <field name="contactPerson" type="string" column="contact_person" length="255"/>
    <field name="mediapath" type="string" column="mediapath" length="255"/>
  </entity>
</doctrine-mapping>

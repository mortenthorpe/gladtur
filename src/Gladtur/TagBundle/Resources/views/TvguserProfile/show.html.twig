{% extends "GladturTagBundle::layout.html.twig" %}
{% block content %}
<h1>&quot;Sted egenskab&quot;</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('tvguserprofile') }}">
            &quot;Sted egenskab&quot; Oversigt
        </a>
    </li>
    <li>
        <a href="{{ path('tvguserprofile_edit', { 'id': entity.id }) }}">
            Rediger
        </a>
    </li>
    <li>
        <form action="{{ path('tvguserprofile_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Slet</button>
        </form>
    </li>
</ul>
{% endblock %}